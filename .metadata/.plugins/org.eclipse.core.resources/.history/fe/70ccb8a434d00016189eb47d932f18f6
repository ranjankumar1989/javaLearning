package JavaBasic;

public class Demo8_MethodOverloading 
{
	public static void main()
	{
		mother m = new Daughter();
		m.sharee();
		
	}

}

class mother
{
	void sharee()
	{
		System.out.println("Simple Saree");
	}

}

class Daughter extends mother
{
	void sharee()
	{
		System.out.println("**** jatre Design*******");
	}
}


