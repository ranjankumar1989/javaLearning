package JavaBasic;

class test
	{
		int i;
		static int j;
	}

class Demo6_static_Nonstatic
	{
		public static void main(String[] args)
		{
			System.out.println("Program started");
		test t1 =  new test();
		t1.i =44;
		t1.j= 66;
		
		test t2 = new test();
		t2.i=99;
		
		
		System.out.println(t1.i);
		System.out.println(t1.j);
		System.out.println(t2.j);
		
		t2.j= 100;
		
		System.out.println(t2.j);
		
			System.out.println("Program Ended");
				
		}
		
	}

